{
	"root": true,
	"parserOptions": {
	  "ecmaVersion": 2018,
	  "sourceType": "module",
	  "parser": "babel-eslint"
	},
	"env": {
	  "browser": true,
	  "node": true,
	  "es6": true,
	  "jquery": true
	},
	"extends": [
	  "eslint:recommended"
	],
	"plugins": [
		"html"
	],
	"rules": {
		"default-case": "warn",
		"comma-dangle": "warn",
		"comma-spacing": [
			"warn", { "before": "false", "after": "true" }
		],
		"func-call-spacing": "warn",
		"no-trailing-spaces": "warn",
		"semi-spacing": [
			"warn", { "before": "false", "after": "true" }
		],
		"space-before-blocks": "warn"
	}
  }